{
  "name": "tailwindui-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "faust build",
    "codegen": "graphql-codegen",
    "dev": "yarn generate && concurrently \"faust dev\" \"yarn watch-codegen\"",
    "generate": "faust generatePossibleTypes",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "start": "faust start",
    "stylesheet": "faust generateGlobalStylesheet",
    "test:ci": "jest --ci",
    "test": "jest --watch --verbose",
    "watch-codegen": "graphql-codegen -w"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@faustwp/cli": "1.1.1",
    "@faustwp/core": "1.1.1",
