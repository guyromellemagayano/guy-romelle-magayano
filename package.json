    "lint:react-utils": "pnpm exec nx run react-utils:lint",
    "lint": "pnpm exec nx run-many -t lint",
    "migrate:nx": "pnpm exec nx migrate latest && pnpm exec nx migrate --run-migrations",
    "prepare": "husky",
    "serve:portfolio": "pnpm exec nx run portfolio:serve",
    "serve:dev:portfolio": "pnpm exec nx run portfolio:serve:development",
    "serve:prod:portfolio": "pnpm exec nx run portfolio:serve:production",
    "serve": "pnpm exec nx run-many -t serve",
    "test:portfolio": "pnpm exec nx run portfolio:test",
    "test:react-components": "pnpm exec nx react-components:test",
    "test:react-hooks": "pnpm exec nx react-hooks:test",
    "test:react-libs": "pnpm exec nx react-libs:test",
    "test:react-utils": "pnpm exec nx react-utils:test",
    "test:watch": "pnpm exec nx run-many -t test --watchAll",
    "test:watch:portfolio": "pnpm exec nx run portfolio:test --watchAll",
    "test:watch:react-components": "pnpm exec nx run react-components:test --watchAll",
    "test:watch:react-hooks": "pnpm exec nx run react-hooks:test --watchAll",
    "test:watch:react-libs": "pnpm exec nx run react-libs:test --watchAll",
    "test:watch:react-utils": "pnpm exec nx run react-utils:test --watchAll",
    "test": "pnpm exec nx run-many -t test"
  },
