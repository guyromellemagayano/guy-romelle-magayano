{
  "extends": [
    "../../../.eslintrc.json"
  ],
  "overrides": [
    {
      "files": [
        "**/*.ts?(x)"
      ],
      "extends": [
        "plugin:@nx/react-typescript"
      ]
    },
    {
      "files": [
        "**/__tests__/**/*.ts?(x)",
        "**/?(*.)+(spec|test).ts?(x)"
      ],
      "extends": [
        "plugin:testing-library/react"
      ],
      "env": {
        "jest": true
      }
    }
  ]
}