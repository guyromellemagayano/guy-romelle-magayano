{
  "name": "wedding-anniversary",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/wedding-anniversary/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/wedding-anniversary"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "wedding-anniversary:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "wedding-anniversary:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "wedding-anniversary:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "wedding-anniversary:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "wedding-anniversary:build:development"
        },
        "production": {
          "buildTarget": "wedding-anniversary:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../coverage/apps/wedding-anniversary"
      }
    }
  }
}