    "lint": "pnpm exec nx run-many --target=lint --all",
    "prepare": "husky",
    "serve:portfolio": "pnpm exec nx serve portfolio --verbose",
    "serve": "pnpm exec nx run-many --target=serve --all",
    "test:react-components": "pnpm exec nx test react-components",
    "test:react-hooks": "pnpm exec nx test react-hooks",
    "test:react-libs": "pnpm exec nx test react-libs",
