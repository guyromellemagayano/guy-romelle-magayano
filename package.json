  "scripts": {
    "build:portfolio": "pnpm exec nx build portfolio --verbose",
    "build": "pnpm exec nx run-many --target=build --all",
    "clean": "pnpm exec nx run-many --target=clean --all",
    "commit": "git-cz",
    "dev:portfolio:spotlight": "pnpm exec spotlight",
    "dev:portfolio": "pnpm exec nx dev portfolio --verbose",
    "dev": "pnpm exec nx run-many --target=dev --all",
    "graphql-codegen:generate": "pnpm exec graphql-codegen -c apps/portfolio/codegen.ts -r dotenv/config -p apps/portfolio/tsconfig.json dotenv_config_path=.env.local",
    "graphql-codegen:watch": "pnpm exec graphql-codegen -w -c apps/portfolio/codegen.ts -r dotenv/config -p apps/portfolio/tsconfig.json dotenv_config_path=.env.local",
    "lint:portfolio": "pnpm exec nx lint portfolio --verbose",
    "lint:react-components": "pnpm exec nx run react-components:lint",
    "lint:react-hooks": "pnpm exec nx run react-hooks:lint",
    "lint:react-libs": "pnpm exec nx run react-libs:lint",
    "lint:react-utils": "pnpm exec nx run react-utils:lint",
    "lint": "pnpm exec nx run-many --target=lint --all",
    "prepare": "husky",
    "serve:portfolio": "pnpm exec nx serve portfolio --verbose",
    "serve": "pnpm exec nx run-many --target=serve --all",
    "test:react-components": "pnpm exec nx test react-components",
    "test:react-hooks": "pnpm exec nx test react-hooks",
    "test:react-libs": "pnpm exec nx test react-libs",
    "test:react-utils": "pnpm exec nx test react-utils",
    "test": "pnpm exec nx run-many --target=test --all"
