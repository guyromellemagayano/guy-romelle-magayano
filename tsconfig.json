{
	"compilerOptions": {
		"target": "es5",
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			]
		},
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": false,
	},
	"include": [
		"next-env.d.ts",
		"global.d.ts",
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": [
		"node_modules",
		"src/__generated__"
	]
}