    "build:portfolio": "pnpm exec nx run portfolio:build",
    "build:dev:portfolio": "pnpm exec nx run portfolio:build:development",
    "build:prod:portfolio": "pnpm exec nx run portfolio:build:production",
    "build": "pnpm exec nx run-many -t build --all",
    "clean": "pnpm exec nx run-many -t clean",
    "commit": "git-cz",
    "dev:portfolio:spotlight": "pnpm exec spotlight",
    "dev:portfolio": "pnpm exec nx run portfolio:dev",
