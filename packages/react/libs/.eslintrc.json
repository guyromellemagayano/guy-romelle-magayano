{
  "extends": ["../../../.eslintrc.json"],
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "extends": ["plugin:@nx/react-typescript"],
      "rules": {}
    },
    {
      "files": ["**/__tests__/**/*.ts?(x)", "**/?(*.)+(spec|test).ts?(x)"],
      "extends": ["plugin:testing-library/react"],
      "env": { "jest": true }
    }
  ],
  "ignorePatterns": ["!**/*"]
}
