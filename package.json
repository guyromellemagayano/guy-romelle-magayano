    "graphql": "^16.8.0",
    "lodash": "^4.17.21",
    "next": "13.4.19",
    "next-sitemap": "^4.2.2",
    "postcss": "^8.4.29",
    "postcss-focus-visible": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "webpack": "^5.88.2"
