  "scripts": {
    "build:portfolio": "pnpm exec nx run portfolio:build",
    "build:dev:portfolio": "pnpm exec nx run portfolio:build:development",
    "build:prod:portfolio": "pnpm exec nx run portfolio:build:production",
    "build:wedding-anniversary": "pnpm exec nx run wedding-anniversary:build",
    "build:dev:wedding-anniversary": "pnpm exec nx run wedding-anniversary:build:development",
    "build:prod:wedding-anniversary": "pnpm exec nx run wedding-anniversary:build:production",
    "build": "pnpm exec nx run-many -t build --all",
    "clean": "pnpm exec nx run-many -t clean",
    "codegen": "graphql-codegen --config codegen.ts",
    "commit": "git-cz",
    "dev:portfolio:spotlight": "pnpm exec spotlight",
    "dev:portfolio": "pnpm exec nx run portfolio:dev",
    "dev:wedding-anniversary": "pnpm exec nx run wedding-anniversary:build:development && pnpm exec nx run wedding-anniversary:serve -c development",
    "lint:portfolio": "pnpm exec nx run portfolio:lint",
    "lint:wedding-anniversary": "pnpm exec nx run wedding-anniversary:lint",
    "lint:react-components": "pnpm exec nx run react-components:lint",
    "lint:react-hooks": "pnpm exec nx run react-hooks:lint",
    "lint:react-libs": "pnpm exec nx run react-libs:lint",
    "lint:react-utils": "pnpm exec nx run react-utils:lint",
    "lint": "pnpm exec nx run-many -t lint",
    "prepare": "husky",
    "serve:portfolio": "pnpm exec nx run portfolio:serve",
    "serve:dev:portfolio": "pnpm exec nx run portfolio:serve:development",
    "serve:prod:portfolio": "pnpm exec nx run portfolio:serve:production",
    "serve:wedding-anniversary": "pnpm exec nx run wedding-anniversary:serve",
    "serve:dev:wedding-anniversary": "pnpm exec nx run wedding-anniversary:serve:development",
    "serve:prod:wedding-anniversary": "pnpm exec nx run wedding-anniversary:serve:production",
    "serve": "pnpm exec nx run-many -t serve",
    "test:portfolio": "pnpm exec nx run portfolio:test",
    "test:wedding-anniversary": "pnpm exec nx run wedding-anniversary:test",
    "test:react-components": "pnpm exec nx react-components:test",
    "test:react-hooks": "pnpm exec nx react-hooks:test",
    "test:react-libs": "pnpm exec nx react-libs:test",
    "test:react-utils": "pnpm exec nx react-utils:test",
    "test:watch": "pnpm exec nx run-many -t test --watchAll",
    "test:watch:portfolio": "pnpm exec nx run portfolio:test --watchAll",
    "test:watch:wedding-anniversary": "pnpm exec nx run wedding-anniversary:test --watch",
    "test:watch:react-components": "pnpm exec nx run react-components:test --watchAll",
    "test:watch:react-hooks": "pnpm exec nx run react-hooks:test --watchAll",
    "test:watch:react-libs": "pnpm exec nx run react-libs:test --watchAll",
    "test:watch:react-utils": "pnpm exec nx run react-utils:test --watchAll",
    "test": "pnpm exec nx run-many -t test"
  },
