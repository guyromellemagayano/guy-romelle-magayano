    "lint": "pnpm exec nx run-many -t lint",
    "migrate:nx": "pnpm exec nx migrate latest && pnpm exec nx migrate --run-migrations",
    "prepare": "husky",
    "serve:portfolio": "pnpm exec nx run portfolio:serve",
    "serve": "pnpm exec nx run-many -t serve",
    "test:portfolio": "pnpm exec nx run portfolio:test",
    "test:react-components": "pnpm exec nx react-components:test",
    "test:shared-libs": "pnpm exec nx shared-libs:test",
    "test:shared-utils": "pnpm exec nx shared-utils:test",
    "test:watch": "pnpm exec nx run-many -t test --watchAll",
    "test:watch:portfolio": "pnpm exec nx run portfolio:test --watchAll",
    "test:watch:react-components": "pnpm exec nx run react-components:test --watchAll",
    "test:watch:shared-libs": "pnpm exec nx run shared-libs:test --watchAll",
    "test:watch:shared-utils": "pnpm exec nx run shared-utils:test --watchAll",
    "test": "pnpm exec nx run-many -t test"
